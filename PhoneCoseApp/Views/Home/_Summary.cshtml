@model Phone

<section class="panel panel-default">
    <h3 class="panel-heading">Model Name: @Model.Name</h3>
    <div class="panel-body">Color: @Model.Color</div>
    <div>
        <a class="btn btn-primary" asp-controller="Home" asp-action="Details" asp-route-Id="@Model.Id">Details</a>
        <a class="btn btn-primary" asp-controller="Home" asp-action="Edit" asp-route-Id="@Model.Id">Edit</a>
    </div>
</section>
